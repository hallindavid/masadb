{
    "repositories": [
        {
            "url": "https://github.com/lotharthesavior/BagItPHP.git",
            "type": "vcs"
        },
        {
            "url": "https://github.com/coyl/git.git",
            "type": "vcs"
        }
    ],
    "require": {
        "coyl/git": "^0.1.7",
        "zendframework/zend-diactoros": "^1.3",
        "league/flysystem": "^1.0",
        "league/oauth2-server": "^5.1",
        "slim/slim": "^3.7.0",
        "lotharthesavior/bagit": "dev-master",
        "guzzlehttp/guzzle": "~6.0",
        "league/flysystem-cached-adapter": "^1.0",
        "php-ds/php-ds": "^1.2"
    },
    "autoload": {
        "psr-4": {
           "Models\\": "Models/",
           "Controllers\\": "Controllers/",
           "Repositories\\": "Repositories/",
           "Helpers\\": "Helpers/"
       }
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload-dev": {
        "psr-4": {
           "Models\\": "Models/",
           "Controllers\\": "Controllers/",
           "Repositories\\": "Repositories/"
       }
    }
}
