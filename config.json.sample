{
    "env": "production",
    "database-address": "/var/www/html/data",
    "domain": "localhost",
    "domain-fallback": "localhost",
    "protocol": "http",
    "displayErrorDetails": "true",
    "case_sensitive": false,
    "addContentLengthHeader": false,
    "cert": "/var/www/html/certs/masadb.csr",
